{
	"env": {
		"es2022": true,
		"node": true
	},
	"globals": {
		"global": "readonly",
		"process": "readonly",
		"Buffer": "readonly",
		"cfg": "writable",
		"db": "writable",
		"scrape": "writable",
		"plugins": "writable",
		"Func": "writable",
		"_": "writable",
		"Api": "writable",
		"Scrape": "writable",
		"pp": "writable",
		"media": "writable",
		"handler": "writable"
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-undef": "off",
		"no-unused-vars": "warn",
		"no-useless-escape": "warn",
		"no-empty": "warn"
	}
}
